/*
  Implements hook_page_attachments().
*/

function cig_pods_page_attachments(array &$attachments) {
  $attachments['#attached']['library'][] = 'cig_pods/hide_return';
}


 /*
	Implements hook_views_pre_render().
 */



function cig_pods_views_pre_render(ViewExecutable $view ) {
	

	if (isset($view) && ($view->storage->id() == 'producer_entry_form')) {
		$view->element['#attached']['library'][] =
		'cig_pods/producer_entry_form';
	}
	if (isset($view) && ($view->storage->id() == 'display_project')) {
		$view->element['#attached']['library'][] =
		'cig_pods/admin_project_display';
	}
	if (isset($view) && ($view->storage->id() == 'producer_display')) {
		$view->element['#attached']['library'][] =
		'cig_pods/admin_details_display';
	}
	if (isset($view) && ($view->storage->id() == 'awardee_organization_display')) {
		$view->element['#attached']['library'][] =
		'cig_pods/admin_details_display';
	}
	if (isset($view) && ($view->storage->id() == 'lab_test_profiles_display')) {
		$view->element['#attached']['library'][] =
		'cig_pods/admin_details_display';
	}
	if (isset($view) && ($view->storage->id() == 'field_assessment')) {
		$view->element['#attached']['library'][] =
		'cig_pods/field_assessment';
	}
	if (isset($view) && ($view->storage->id() == 'methods_display')) {
		$view->element['#attached']['library'][] =
		'cig_pods/awardee_methods_details_display';
	}
	if (isset($view) && ($view->storage->id() == 'display_irrigation')) {
		$view->element['#attached']['library'][] =
		'cig_pods/admin_project_display';
	}
	if (isset($view) && ($view->storage->id() == 'display_operation')) {
		$view->element['#attached']['library'][] =
		'cig_pods/admin_project_display';
	}
	if (isset($view) && ($view->storage->id() == 'shmu_display')) {
		$view->element['#attached']['library'][] =
		'cig_pods/admin_details_display';
	}
	if (isset($view) && ($view->storage->id() == 'lab_results_display')) {
		$view->element['#attached']['library'][] =
		'cig_pods/awardee_lab_results_display';
	}
	if (isset($view) && ($view->storage->id() == 'soil_health_sample_display')) {
		$view->element['#attached']['library'][] =
		'cig_pods/awardee_soil_sample_display';
	}
	if (isset($view) && ($view->storage->id() == 'field_assessment')) {
		$view->element['#attached']['library'][] =
		'cig_pods/awardee_soil_sample_display';
	}
	if (isset($view) && ($view->storage->id() == 'rangeland_assessment_display')) {
		$view->element['#attached']['library'][] =
		'cig_pods/range_assessment_display';
	}
	
}

